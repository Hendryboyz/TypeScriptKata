{
  "name": "typescriptkata",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll",
    "start:dev": "nodemon",
    "start": "npm run build && node build/index.js",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "build": "rmraf ./build && tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hendryboyz/TypeScriptKata.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hendryboyz/TypeScriptKata/issues"
  },
  "homepage": "https://github.com/Hendryboyz/TypeScriptKata#readme",
  "devDependencies": {
    "@shopify/eslint-plugin": "^41.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "onchange": "^7.1.0",
    "prettier": "^2.5.1",
    "rmraf": "^1.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "eslint-plugin-jest": "^25.3.4",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2"
  }
}
